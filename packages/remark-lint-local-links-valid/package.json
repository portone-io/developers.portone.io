{
  "name": "@portone-io/remark-lint-local-links-valid",
  "version": "0.1.1",
  "repository": "https://github.com/portone-io/developers.portone.io",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "dist/index.js",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build": "tsup",
    "prepublishOnly": "pnpm build"
  },
  "tsup": {
    "entry": ["src/index.ts"],
    "format": ["cjs", "esm"]
  },
  "dependencies": {
    "unified-lint-rule": "^3.0.0",
    "unist-util-visit": "^5.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.12.7",
    "tsup": "^8.0.2"
  },
  "packageManager": "pnpm@9.0.5",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "access": "public"
  }
}
