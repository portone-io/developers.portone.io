---
import LayoutBase from "./LayoutBase.astro";

interface Props {
  title?: string;
  description?: string | undefined;
  slug?: string | undefined;
  ogType?: string | undefined;
}

const { title, slug, ogType } = Astro.props;
---

<LayoutBase
  lang="en"
  title={title ?? "PortOne Developers"}
  slug={slug}
  ogType={ogType}
>
  <slot name="og" slot="og" />
  <slot />
</LayoutBase>
