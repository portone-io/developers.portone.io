---
import LayoutBase from "./LayoutBase.astro";

interface Props {
  title?: string;
  description?: string | undefined;
  navAsMenu?: boolean | undefined;
}

const { title, description, navAsMenu } = Astro.props;
---

<LayoutBase
  lang="ko"
  title={title ?? "PortOne 개발자센터"}
  description={description}
  navAsMenu={navAsMenu}
>
  <slot />
</LayoutBase>
